	<!-- Page header -->
	<div class="page-header">
		<div class="system-alerts">
			<div class="row">
				<div class="col-md-12">
					<% if ( currentUser.accounttype === 'administrator' && (systemMessages && systemMessages.length !== 0) ) {
						for (var i = 0; i < systemMessages.length; i++) { %>
							<%- app.alert(systemMessages[i], 'system');%>
						<% }
					} %>
				</div>
			</div>
		</div>
	</div>
	<!-- /page header -->
