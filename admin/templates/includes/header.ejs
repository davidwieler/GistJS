		const postId = '<%- postData._id%>';
		const autoSaveTimer = '<%- cms.cmsDetails.autoSaveTimer%>';
		const coreVersion = '<%- cms.cmsDetails.coreVersion%>';
		let subscriptionStatus = '';
	</script>
	<script type="text/javascript" src="/gistjs-assets/js/core/libraries/jquery.min.js"></script>
	<script type="text/javascript" src="/gistjs-assets/js/core/libraries/bootstrap.min.js"></script>
	<script type="text/javascript" src="/gistjs-assets/js/core/libraries/wysihtml.min.js"></script>
