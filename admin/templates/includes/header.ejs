	<script type="text/javascript" src="/gistjs-assets/js/core/app.js"></script>
	<script type="text/javascript" src="/gistjs-assets/js/core/handlers.js"></script>
	<script type="text/javascript" src="/gistjs-assets/js/core/helperFunctions.js"></script>
