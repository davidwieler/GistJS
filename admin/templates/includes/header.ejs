	<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.12.2/js/bootstrap-select.min.js"></script>
	<!-- /core JS files -->

	<!-- Plugin CSS & JS files -->

<%
	for (var i = plugins.length - 1; i >= 0; i--) {
		var cssFile = plugins[i].pluginInfo.css;
		var jsFile = plugins[i].pluginInfo.init;
		var path = plugins[i].pluginInfo.path;
		var render = plugins[i].pluginInfo.render;
		if (cssFile) {
			%><link href="/plugins/<%- path %>/<%- cssFile %>" rel="stylesheet" type="text/css"><%
		}

		if (render === 'header') {

			%><script type="text/javascript" src="/plugins/<%- path %>/<%- jsFile %>"></script><%
		}
	}
%>
	<!-- /plugin CSS & JS files -->
</head>

